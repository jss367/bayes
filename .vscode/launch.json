    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Different Priors",
            "type": "debugpy",
            "request": "launch",
            "program": "different_priors.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: ECMO",
            "type": "debugpy",
            "request": "launch",
            "program": "ecmo.py",
            "console": "integratedTerminal"
        }
    ]
}
